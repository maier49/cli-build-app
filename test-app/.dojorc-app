{
	"build-app": {
		"dojoLoaderConfig": {
			"baseUrl": "node_modules",
			"has": {
				"dojo-config-api": 0
			}
		}
		"locale": "en",
		"supportedLocales": [ "es" ],
		"features": {
			"foo": true,
			"bar": false
		},
		"bundles": {
			"widgets": [
				"src/Bar",
				"src/Baz"
			],
			"lazy": [
				"src/LazyWidget"
			]
		},
		"build-time-render": {
			"root": "app"
		}
	}
}
