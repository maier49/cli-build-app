{
	"build-app": {
		"dojoLoaderConfig": {
			"baseUrl": ".",
			"packages": [
				{
					"name": "dojo",
					"location": "node_modules/dojo",
					"lib": "."
				}
			],
			"has": {
				"dojo-config-api": 0
			},
			"async": true
		},
		"locale": "en",
		"supportedLocales": [ "es" ],
		"features": {
			"foo": true,
			"bar": false
		},
		"bundles": {
			"widgets": [
				"src/Bar",
				"src/Baz"
			],
			"lazy": [
				"src/LazyWidget"
			]
		},
		"build-time-render": {
			"root": "app"
		}
	}
}
